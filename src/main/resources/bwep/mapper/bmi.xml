<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bmi">

<!-- 사용자의 정보를 받아 DB 에 저장하는 기능!! -->
<insert id="addBmi" parameterType="bwep.vo.BmiVO">
	INSERT INTO bmi_t (m_nick, b_kg, b_cm, b_age)
	VALUES (#{m_nick}, #{b_kg}, #{b_cm}, #{b_age})
</insert>

</mapper>